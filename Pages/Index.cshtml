@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
  <span class="fs-1">
  Welcome to SJC Voters portal
  </span>
</div>
    <div id="carouselExample" class="carousel slide">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="~/images/vote1.jpg" class="d-block w-100" alt="Image 1" style="width: 800px; height: 600px">
        </div>
        <div class="carousel-item">
          <img src="~/images/vote2.jpg" class="d-block w-100" alt="Image 2" style="width: 800px; height: 600px">
        </div>
        <div class="carousel-item">
          <img src="~/images/vote3.jpg" class="d-block w-100" alt="Image 3" style="width: 800px; height: 600px">
        </div>
        <div class="carousel-item">
          <img src="~/images/vote4.jpeg" class="d-block w-100" alt="Image 4" style="width: 800px; height: 600px">
        </div>
        <div class="carousel-item">
          <img src="~/images/vote5.webp" class="d-block w-100" alt="Image 5" style="width: 800px; height: 600px">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
